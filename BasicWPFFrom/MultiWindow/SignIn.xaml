<Window x:Class="MultiWindow.SignIn"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MultiWindow"
        mc:Ignorable="d"
        Title="SignIn" Height="450" Width="800">
    <Grid>
        <Label Content="Plase enter your user name and password" x:Name="lable"
                   HorizontalAlignment="Left" VerticalAlignment="Top" Margin="150,50, 0, 0"/>
        <Label Content="Email : " HorizontalAlignment="Left" VerticalAlignment="Top" Margin="150,80,0,0" />

        <Label Content="Password : " HorizontalAlignment="Left" VerticalAlignment="Top" Margin="150,120,0,0" />



    </Grid>
</Window>
